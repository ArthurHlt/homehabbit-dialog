<?xml version="1.0" encoding="UTF-8"?>
<dialog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform"
        xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd">
    <flow/>
    <entities xdt:Transform="Insert">
        <entity name="Place">
            <value name="Kitchen" value="Kitchen">
                <grammar>
                    <item>tchen</item>
                    <item>kitchen</item>
                </grammar>
            </value>
            <value name="Living" value="Living">
                <grammar>
                    <item>living room</item>
                    <item>living</item>
                </grammar>
            </value>
            <value name="Bedroom" value="Bedroom">
                <grammar>
                    <item>bedroom</item>
                    <item>bedroom one</item>
                </grammar>
            </value>
            <value name="Toilets" value="Toilets">
                <grammar>
                    <item>Toilet</item>
                    <item>Toilets</item>
                </grammar>
            </value>
            <value name="Bathroom" value="Bathroom">
                <grammar>
                    <item>Bath</item>
                    <item>Bathroom</item>
                </grammar>
            </value>
        </entity>
        <entity name="Device">
            <value name="Light" value="Light">
                <grammar>
                    <item>Light</item>
                    <item>Lights</item>
                    <item>Power</item>
                </grammar>
            </value>
            <value name="Coffee" value="CoffeeMaker">
                <grammar>
                    <item>Coffee maker</item>
                    <item>Coffee</item>
                </grammar>
            </value>
            <value name="Projector" value="Projector">
                <grammar>
                    <item>projector</item>
                    <item>jector</item>
                </grammar>
            </value>
            <value name="Tv" value="Tv">
                <grammar>
                    <item>television</item>
                    <item>tv</item>
                </grammar>
            </value>
        </entity>
        <entity name="Action">
            <value name="Turn" value="Turn">
                <grammar>
                    <item>Switch</item>
                    <item>prepare</item>
                    <item>Turn</item>
                    <item>Power</item>
                </grammar>
            </value>
            <value name="Prepare" value="Prepare">
                <grammar>
                    <item>prepare</item>
                </grammar>
            </value>
            <value name="Change" value="Change">
                <grammar>
                    <item>Change</item>
                    <item>go to</item>
                </grammar>
            </value>
        </entity>

        <entity name="ActionValue">
            <value name="On" value="On">
                <grammar>
                    <item>On</item>
                </grammar>
            </value>
            <value name="Off" value="Off">
                <grammar>
                    <item>off</item>
                    <item>stop</item>
                </grammar>
            </value>
        </entity>
    </entities>
</dialog>